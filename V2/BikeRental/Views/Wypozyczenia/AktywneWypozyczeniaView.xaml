<local:WszystkieViewBase x:Class="BikeRental.Views.AktywneWypozyczeniaView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BikeRental.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <DataGrid AutoGenerateColumns="False" ItemsSource="{Binding List}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" Binding="{Binding WypozyczenieId}"/>
                <DataGridTextColumn Header="Imie" Binding="{Binding KlientImie}"/>
                <DataGridTextColumn Header="Nazwisko" Binding="{Binding KlientNazwisko}"/>
                <DataGridTextColumn Header="Kod floty" Binding="{Binding KodFloty}"/>
                <DataGridTextColumn Header="Start" Binding="{Binding StartUtc}"/>
                <DataGridTextColumn Header="Koniec" Binding="{Binding KoniecUtc}"/>
                <DataGridTextColumn Header="Stacja poczatkowa" Binding="{Binding NazwaStacjiPoczatkowej}"/>
                <DataGridTextColumn Header="Stacja koncowa" Binding="{Binding NazwaStacjiKoncowej}"/>
                <DataGridTextColumn Header="Szerokosc geo. start" Binding="{Binding StartSzerGeo}"/>
                <DataGridTextColumn Header="Dlugosc geo. start" Binding="{Binding StartDlugGeo}"/>
                <DataGridTextColumn Header="Szerokosc geo. koniec" Binding="{Binding KoniecSzerGeo}"/>
                <DataGridTextColumn Header="Dlugosc geo. koniec" Binding="{Binding KoniecDlugGeo}"/>
                <DataGridTextColumn Header="Odleglosc[KM]" Binding="{Binding OdlegloscKm}"/>
                <DataGridTextColumn Header="PlanCenowyIdSnapshot" Binding="{Binding PlanCenowyIdSnapshot}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</local:WszystkieViewBase>
