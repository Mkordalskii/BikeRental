<local:WszystkieViewBase x:Class="BikeRental.Views.WypozyczenieOplataView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BikeRental.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <DataGrid AutoGenerateColumns="False" ItemsSource="{Binding List}">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" Binding="{Binding OplataId}"/>
                <DataGridTextColumn Header="Id wypozyczenia" Binding="{Binding WypozyczenieId}"/>
                <DataGridTextColumn Header="Typ" Binding="{Binding Typ}"/>
                <DataGridTextColumn Header="Minuty" Binding="{Binding IloscMin}"/>
                <DataGridTextColumn Header="Stawka" Binding="{Binding Stawka}"/>
                <DataGridTextColumn Header="Kwota" Binding="{Binding Kwota}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</local:WszystkieViewBase>
