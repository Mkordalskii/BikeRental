<UserControl x:Class="BikeRental.Views.AktywneWypozyczeniaView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BikeRental.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <DataGrid AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Id" />
                <DataGridTextColumn Header="Id klienta" />
                <DataGridTextColumn Header="Id roweru" />
                <DataGridTextColumn Header="Start" />
                <DataGridTextColumn Header="Koniec" />
                <DataGridTextColumn Header="Id stacji poczatkowej" />
                <DataGridTextColumn Header="Id stacji koncowej" />
                <DataGridTextColumn Header="Szerokosc geo. start" />
                <DataGridTextColumn Header="Dlugosc geo. start" />
                <DataGridTextColumn Header="Szerokosc geo. koniec" />
                <DataGridTextColumn Header="Dlugosc geo. koniec" />
                <DataGridTextColumn Header="Odleglosc[KM]" />
                <DataGridTextColumn Header="Plan cenowy Id" />
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
